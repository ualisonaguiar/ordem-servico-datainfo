<?php

namespace InepZend\Form;

use InepZend\Form\AbstractSpecificTest;

class HtmlSpecificTest extends AbstractSpecificTest
{

    public function testAddHtml()
    {
        $this->assertInstanceOf('InepZend\FormGenerator\FormGenerator', self::getInstanceOf()->addHtml('strValue', 'HtmlNew'));
    }
    
    public function testAddHtml2()
    {
        $this->assertInstanceOf('InepZend\FormGenerator\Element\Html', self::getInstanceOf()->addHtml('strValue', 'HtmlNew', false));
    }
    
    public function testAddHtml3()
    {
        $this->assertInstanceOf('InepZend\FormGenerator\Element\Html', self::getInstanceOf()->addHtml('strValue', 'HtmlNew', false));
    }

}
