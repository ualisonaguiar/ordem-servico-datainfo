#barra-brasil-v3 {
    background: none repeat scroll 0pt 0pt rgb(0, 80, 15); 
    clear: both; 
    height: 28px; 
    min-width: 992px; 
    overflow: hidden; 
    position: relative;
}

#barra-brasil-v3 div {
    background: url(../images/barra-brasil-v3-bgx.gif) repeat-x scroll 0 bottom transparent;
    display: block;
    height: 28px;
    width: 50%;
}

#barra-brasil-v3 div .imagemGov {
    background: url(../images/barra-brasil-v3-verde.gif) no-repeat scroll right top transparent;
    height: 24px;
    left: 50%;
    margin-left: -490px;
    position: absolute;
    top: 4px;
    width: 992px;
}

#barra-brasil-v3 div .brasilgov {
    border: 0 none !important;
    display: block;
    float: right;
    height: 24px;
    width: 100px;
}

#barra-brasil-v3 div .brasilgovAI {
    background: url(data:image/png;base64,) no-repeat;
    *background: url(../images/ai.png) no-repeat;
    width: 161px;
    height: 24px;
    float: right;
    display: block;
    border: 0!important;
    margin: 0 30px 0 0;
}
