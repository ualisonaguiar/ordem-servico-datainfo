.btn-inep {
    background: url(data:image/svg+xml;base64,) #53bce2;
    background: -moz-linear-gradient(45deg,transparent 0%,transparent 74%,#cce7f5 74%,#cce7f5 80%,transparent 80%,transparent 82%,#cce7f5 82%,#cce7f5 85%,transparent 85%,transparent 90%,#cce7f5 90%,#cce7f5 91%,transparent 91%,transparent 100%) no-repeat border-box, linear-gradient(to bottom,#53bce2 0%,#53bce2 80%,#54abbe 100%) no-repeat border-box;
    background: -webkit-linear-gradient(45deg,transparent 0%,transparent 74%,#cce7f5 74%,#cce7f5 80%,transparent 80%,transparent 82%,#cce7f5 82%,#cce7f5 85%,transparent 85%,transparent 90%,#cce7f5 90%,#cce7f5 91%,transparent 91%,transparent 100%) no-repeat border-box, linear-gradient(to bottom,#53bce2 0%,#53bce2 80%,#54abbe 100%) no-repeat border-box;
    background: -o-linear-gradient(45deg,transparent 0%,transparent 74%,#cce7f5 74%,#cce7f5 80%,transparent 80%,transparent 82%,#cce7f5 82%,#cce7f5 85%,transparent 85%,transparent 90%,#cce7f5 90%,#cce7f5 91%,transparent 91%,transparent 100%) no-repeat border-box, linear-gradient(to bottom,#53bce2 0%,#53bce2 80%,#54abbe 100%) no-repeat border-box;
    background: -ms-linear-gradient(45deg,transparent 0%,transparent 74%,#cce7f5 74%,#cce7f5 80%,transparent 80%,transparent 82%,#cce7f5 82%,#cce7f5 85%,transparent 85%,transparent 90%,#cce7f5 90%,#cce7f5 91%,transparent 91%,transparent 100%) no-repeat border-box, linear-gradient(to bottom,#53bce2 0%,#53bce2 80%,#54abbe 100%) no-repeat border-box;
    background: linear-gradient(45deg,transparent 0%,transparent 74%,#cce7f5 74%,#cce7f5 80%,transparent 80%,transparent 82%,#cce7f5 82%,#cce7f5 85%,transparent 85%,transparent 89%,#cce7f5 89%,#cce7f5 91%,transparent 91%,transparent 100%) no-repeat border-box, linear-gradient(to bottom,#53bce2 0%,#53bce2 80%,#54abbe 100%) no-repeat border-box;
    box-shadow: 1px 1px 1px #aaa;
    color: #fff;
    font-family: Arial, sans-serif;
    font-size: 13px;
    font-weight: bold;
    text-shadow: 1px 1px 0px #164977;
    padding: 0px 5px;
    border-radius: 9px;
    border: 1px solid #ddd;
    text-indent: 8px;
    filter: progid:DXImageTransform.Microsoft.Shadow(color=#aaa,direction=45); /*IE*/
    zoom: 1;
    text-align: left;
    margin-top: 4px;
    height: 25px;
}